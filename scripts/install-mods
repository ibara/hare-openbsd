#!/bin/sh
modules="ascii
bufio
bytes
crypto
datetime
dirs
encoding
endian
errors
fmt
fnmatch
format
fs
getopt
glob
hare
hash
io
linux
log
math
mime
net
os
path
regex
rt
shlex
slices
sort
strconv
strings
strio
temp
time
types
unix
uuid"
